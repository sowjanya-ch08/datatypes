    <script>
        let number = parseInt(prompt("enter a number"))
        if(number %3==0 && number %2==0){
            document.write("the number is divisible both by 2 & 3")

        }else{
            document.write("the number is divisible not divisible by 2 & 3")
        }
    </script>
</body>
