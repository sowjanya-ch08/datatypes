        let a = "sai";
        let b =" kumar";
        console.log(`react.js student = ${a+" "+b}`);
        document.write(`react.js student = ${a+" "+b}`)
let a =1
        console.log("hi+a+a");
        document.write('hi+a+a',"<br>")
        document.write(`hi+${a+a}`)
        console.log(`hi+${a+a}`)

    </script>
</body>
